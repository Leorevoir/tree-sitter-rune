{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "rune",
      "camelcase": "Rune",
      "title": "Rune",
      "scope": "source.rune",
      "file-types": [
        "ru"
      ],
      "injection-regex": "^rune$",
      "class-name": "TreeSitterRune"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Rune tree-sitter syntax parser",
    "authors": [
      {
        "name": "Leorevoir",
        "email": "leo.quinzler@epitech.eu"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-rune"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}